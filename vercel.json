{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*.js", 
      "use": "@vercel/node",
      "config":{
        "includeFiles": [
          "node_modules/.prisma/client/query-engine-rhel-openssl-*.node",
          "node_modules/.prisma/client/schema.prisma"
        ]
          }}
    
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/routes/TSroutes.js"
    }
  ]
}